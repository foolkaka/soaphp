  "authors": [
    {
      "name": "foolkaka",
      "email": "foolkaka@hotmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/lumen-framework": "5.2.*",
    "vlucas/phpdotenv": "~2.2",
    "foolkaka/soaphp-framework":"v1.0.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~4.0"
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
